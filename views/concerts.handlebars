<div class="row">
  {{! Concert Connect}}
  <div class="container">
    <ul>
      {{#each concert}}
        <div class="concert-card">
          <p>{{this.dataValues.eventName}}</p>
          <p>{{this.dataValues.venueName}}</p>
          <p>{{this.dataValues.venueAddress}}</p>
          <p>{{this.dataValues.eventDate}}</p>
          <p>Buy Tickets Here:</p> <a href = {{this.dataValues.ticketURL}}>{{this.dataValues.ticketURL}}</a>
          <p>Do you wanna go?</p>  
          <form action="concert">
            <input type="hidden" name=concertID value={{this.dataValues.concert_id}}>
            <input type="hidden" name=userID value={{this.dataValues.user_id}}>
            <button type="submit" class="waves-effect waves-light btn">Yes!</button>
          </form>
        </div>
      {{/each}}
    </ul>
  </div>
</div>